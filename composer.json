{
    "name": "fadlisaad/laravel-log-viewer",
    "description": "Custom log viewer for laravel",
    "type": "library",
    "keywords": ["laravel-admin", "log", "viewer"],
    "homepage": "https://github.com/fadlisaad/laravel-log-viewer",
    "license": "MIT",
    "authors": [
        {
            "name": "Fadli Saad",
            "email": "fadlisaad@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6",
        "laravel-admin-ext/config": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Admin\\LogViewer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Admin\\LogViewer\\LogViewerServiceProvider"
            ]
        }
    }
}
